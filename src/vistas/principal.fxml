<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<BorderPane fx:id="mainPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.main">
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="botonFaseGrupos" mnemonicParsing="false" onAction="#cargarPanelGrupos" text="Fase de grupos">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="botonEliminatorias" mnemonicParsing="false" onAction="#cargarPanelEliminatorias" text="Eliminatorias">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="botonEquipos" mnemonicParsing="false" text="Equipos">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="botonEstadios" mnemonicParsing="false" text="Estadios">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets top="5.0" />
         </BorderPane.margin>
      </HBox>
   </top>
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="logo" fitHeight="512.0" fitWidth="512.0" layoutX="144.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true">
            </ImageView>
         </children>
      </Pane>
   </center>
</BorderPane>
